USE rxweb_preetgandhi;

CREATE TABLE employees 
(
	Employee_Id int PRIMARY KEY,
	FirstName char(20),
	LastName char(20),
	Email varchar(30),
	PhoneNumber numeric(10),
	Hire_Date date,
	Job_Id int NOT NULL UNIQUE FOREIGN KEY REFERENCES job(JobId),
	Salary numeric(7),
	Commission numeric(7),
	Manager_Id int ,
	Department_Id int NOT NULL FOREIGN KEY REFERENCES Departments(DepartmentId),
);
